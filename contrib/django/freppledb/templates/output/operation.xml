<?xml version="1.0" encoding="UTF-8" ?>
{% load i18n %}{% load l10n %}{% localize off %}
<graph yAxisName='{% trans "units" %}' 
 decimalPrecision='0' numDivLines='4' 
 showValues='0' formatNumberScale='0' animation='0'>
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}</categories>
<dataset seriesName='{% filter force_escape %}{% trans "total starts" %}{% endfilter %}' color='AFD8F8' showValues='0'>
{% for j in total_start %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "total ends" %}{% endfilter %}'  color='F6BD0F' showValues='0'>
{% for j in total_end %}<set value='{{j}}' />
{% endfor %}</dataset>
{% endlocalize %}
</graph>
