{% load i18n %}
<graph yAxisName='{% trans "units" %}' 
 decimalPrecision='0' numDivLines='4' 
 showValues='0' formatNumberScale='0' animation='0'>
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}
</categories>
<dataset seriesName='{% trans "total starts" %}' color='AFD8F8' showValues='0'>
{% for j in total_start %}<set value='{{j}}' />
{% endfor %}
</dataset>
<dataset seriesName='{% trans "total ends" %}'  color='F6BD0F' showValues='0'>
{% for j in total_end %}<set value='{{j}}' />
{% endfor %}
</dataset>
</graph>
