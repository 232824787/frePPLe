{% load i18n %}
<graph yAxisName='{% trans "units" %}' 
 decimalPrecision='0' numDivLines='4' 
 showValues='0' formatNumberScale='0' animation='0'>
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}
</categories>
<dataset seriesName='{% trans "total forecast" %}'  color='F6BD0F' showValues='0'>
{% for j in total %}<set value='{{j}}' />
{% endfor %}
</dataset>
<dataset seriesName='{% trans "planned net forecast" %}'  color='8BBA00' showValues='0'>
{% for j in planned %}<set value='{{j}}' />
{% endfor %}
</dataset>
<dataset seriesName='{% trans "orders" %}' color='AFD8F8' showValues='0'>
{% for j in orders %}<set value='{{j}}' />
{% endfor %}
</dataset>
</graph>

