<?xml version="1.0" encoding="UTF-8" ?>
{% load i18n %}{% load l10n %}{% localize off %}
<graph shownames='1' showvalues='0'  
  decimalPrecision='2' limitsDecimalPrecision='0' showShadow='0'
  divLineDecimalPrecision='0' connectNullData='0' numDivLines='4' 
  PYAxisName='{% filter force_escape %}{% trans "produced and consumed" %}{% endfilter %}' 
  SYAxisName='{% filter force_escape %}{% trans "start inventory" %}{% endfilter %}' 
  formatNumberScale='1' bgcolor='f1f1f1' animation='0' 
  showalternateHGridColor='1' alternateHGridColor='eeeeee'>
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}</categories>
<dataset seriesName='{% filter force_escape %}{% trans "produced" %}{% endfilter %}' parentYAxis='P' color='AFD8F8' showValues='0'>
{% for j in produced %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "consumed" %}{% endfilter %}' parentYAxis='P'  color='F6BD0F' showValues='0'>
{% for j in consumed %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "start inventory" %}{% endfilter %}' color='8BBA00' showValues='0' 
  parentYAxis='S' anchorSides='10' anchorBorderColor='8BBA00'>
{% for j in startoh %}<set value='{{j}}' />
{% endfor %}</dataset>
{% endlocalize %}
</graph>
