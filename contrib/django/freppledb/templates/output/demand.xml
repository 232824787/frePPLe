<?xml version="1.0" encoding="UTF-8" ?>
{% load i18n %}{% load l10n %}{% localize off %}
<graph shownames='1' showvalues='0'  
  decimalPrecision='2' limitsDecimalPrecision='0' 
  divLineDecimalPrecision='0' connectNullData='0' 
  PYAxisName='Demand and supply' SYAxisName='Backlog' numDivLines='4' 
  formatNumberScale='1' showShadow='0' bgColor='f1f1f1' 
  showalternateHGridColor='1' alternateHGridColor='eeeeee'
  animation='0' >
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}</categories>
<dataset seriesName='{% trans "demand" %}' parentYAxis='P' color='AFD8F8' showValues='0'>
{% for j in demand %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% trans "supply" %}' parentYAxis='P'  color='F6BD0F' showValues='0'>
{% for j in supply %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% trans "backlog" %}' color='8BBA00' showValues='0' 
  parentYAxis='S' anchorSides='10' anchorBorderColor='8BBA00'>
{% for j in backlog %}<set value='{{j}}' />
{% endfor %}</dataset>
{% endlocalize %}
</graph>
