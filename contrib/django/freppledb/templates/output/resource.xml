<?xml version="1.0" encoding="UTF-8" ?>
{% load i18n %}{% load l10n %}{% localize off %}
<graph yAxisName='{% filter force_escape %}{{units}}{% endfilter %}' 
 decimalPrecision='2' numDivLines='4' 
 showValues='0' formatNumberScale='0' animation='0'>
<categories>
{% for j in buckets %}<category name='{{j.name}}' {% if not forloop.counter|divisibleby:axis_nth %}showName="0"{% endif %}/>
{% endfor %}</categories>
<dataset seriesName='{% filter force_escape %}{% trans "unavailable" %}{% endfilter %}'  color='F6BD0F' showValues='0'>
{% for j in unavailable %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "setup" %}{% endfilter %}'  color='2B95EC' showValues='0'>
{% for j in setup %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "load" %}{% endfilter %}' color='AFD8F8' showValues='0'>
{% for j in load %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "free" %}{% endfilter %}' color='8BBA00' showValues='0'>
{% for j in free %}<set value='{{j}}' />
{% endfor %}</dataset>
<dataset seriesName='{% filter force_escape %}{% trans "overload" %}{% endfilter %}' color='FF0000' showValues='0'>
{% for j in overload %}<set value='{{j}}' />
{% endfor %}</dataset>
{% endlocalize %}
</graph>
