<plan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!--
  This file is a frePPLe command file that is automatically read when
  the library is being initialized. It can be used to set run some
  initialization logic and/or load extension modules.

  If no file "$FREPPLE_HOME/init.xml" is found, no startup commands are
  executed. If the file exists but is unreadable, a warning message will be
  printed.

  If the contents of this file has validation errors, the execution is
  aborted.
  -->
  <!--
  <logfile>frepple.log</logfile>
  -->
  <commands>
    <verbose>false</verbose>
    <abortonerror>false</abortonerror>
    <!-- Python interpreter module -->
    <command xsi:type="command_loadlib" filename="mod_python.so" />
    <!-- Forecast module -->
    <command xsi:type="command_loadlib" filename="mod_forecast.so">
      <parameter name="Customer_Then_Item_Hierarchy" value="true" />
      <parameter name="Match_Using_Delivery_Operation" value="false" />
      <parameter name="Net_Early" value="P4W" />
      <parameter name="Net_Late" value="P4W" />
    </command>
    <!-- Webservice module -->
    <!--
    <command xsi:type="command_loadlib" filename="mod_webservice.so">
      <parameter name="port" value="6262" />
      <parameter name="threads" value="5" />
    </command>
    -->
    <!-- Linear Programming solver module -->
    <!--
    <command xsi:type="command_loadlib" filename="mod_lp_solver.so" />
    -->
  </commands>
</plan>
